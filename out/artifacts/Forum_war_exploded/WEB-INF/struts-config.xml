<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
    <form-beans>
        <form-bean name="uploadFileForm" type="com.SE3Forum.fzu.Struct.FormBeans.UploadForm" />
    </form-beans>
    <global-exceptions />
    <global-forwards />
    <action-mappings>
        <action path="/upload" forward="/uploadFile.jsp"></action>
        <action attribute="loginForm" input="/index.jsp" name="uploadFileForm" path="/uploadAction"
                type="com.SE3Forum.fzu.Struct.Actions.uploadFileAction">
            <forward name="success" path="index.jsp"/>
                </action>
    </action-mappings>

</struts-config>